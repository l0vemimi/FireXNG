{
  "name": "SearxNG",
  "description": "SearxNG Search Engine",
  "manifest_version": 2,
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "searxng@firexng.org"
    }
  },
  "hidden": true,
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "@searxng",
      "name": "SearxNG",
      "search_url": "https://searx.tiekoetter.com//search",  
      "search_url_get_params": "q={searchTerms}",
      "suggest_url": "https://searx.tiekoetter.com//osjson",  
      "suggest_url_get_params": "query={searchTerms}"
    }
  }
}
